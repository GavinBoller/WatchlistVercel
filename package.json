{
  "name": "watchlist",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "dev": "concurrently \"tsc -p server/tsconfig.json --watch\" \"node --import ts-node/esm server/index.ts\" \"vite\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.{ts}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"server/**/*.{ts}\"",
    "migrate": "drizzle-kit push --config=./server/drizzle.config.ts",
    "preview": "vite preview",
    "studio": "drizzle-kit studio --host localhost --port 3001"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.1",
